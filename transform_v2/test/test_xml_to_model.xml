<project>
  <target name="run-tests">
    <epsilon.eunit src="test_xml_to_model.eunit"/>
  </target>
  <target name="loadModels">
      <epsilon.xml.loadModel 
      name="XML"
      file="input_xml/TouchSwitch_netlist.xml"
      read="true" store="false"/>
      <epsilon.emf.loadModel 
      name="touchswitch_model_expected"
      modelfile="expected_output_models/touchswitch.model"
      metamodelfile="../metamodel_v2.ecore"
      read="true" store="false"/>
      <epsilon.emf.loadModel 
      name="OUT"
      modelfile="transformed.model"
      metamodelfile="../metamodel_v2.ecore"
      read="false" store="false"/>
  </target>
  <target name="xml2model" depends="loadModels">
  <epsilon.etl src="../xml_to_model.etl">
  <model ref="XML"/>
  <model ref="OUT"/>
  </epsilon.etl>
  </target>
</project>